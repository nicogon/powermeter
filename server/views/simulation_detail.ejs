<div class="row">
  <div class="col-md-12">
    <div class="card card-small">
      <div class="card-header border-bottom">
        <h6 class="m-0">Detalle Simulación</h6>
      </div>

      <div class="card-body pt-0 row">
        <table class="table mb-0">
          <thead class="bg-light">
            <tr>
              <th scope="col" class="border-0">Nombre</th>
              <th scope="col" class="border-0">Porcentaje</th>
              <th scope="col" class="border-0">Consumo</th>
              <th scope="col" class="border-0">Costo Consumo Total</th>
              <th scope="col" class="border-0">Duración</th>
              <th scope="col" class="border-0"></th>
            </tr>
          </thead>
          <tbody>
            <!-- <tfoot> -->
            <thead class="bg-light">
              <tr>
                <td><b><%=simulation.name%></b></td>
                <td><b>94,3%</b></td>
                <td><b><%=simulation.kwhCost%> Kw</b></td>
                <td><b>$<%=simulation.totalCost%></b></td>
                <td><b><%=simulation.durationInHours / 24%> días</b></td>
              </tr>
              <!-- </tfoot> -->
            </thead>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
